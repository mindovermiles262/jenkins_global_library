@Library('my-jgl') _

pipeline {
  agent any
  stages {
    stage('Valid Branches') {
      steps {
        withEnv(["BRANCH_NAME=feature/xyz"]) {
          script {
            echo "Branch (via ENV): ${env.BRANCH_NAME}"
            dataeng.verifyBranchName()          //=> Branch feature/xyz is valid
          }
        }
      }
    }
  }
}
